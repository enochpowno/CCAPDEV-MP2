{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "class-methods-use-this": "off",
      "no-empty": "off",
      "no-underscore-dangle": "off",
      "no-param-reassign": "off",
      "prefer-destructuring": "off",
      "no-unused-vars": "off"
    }
}
