{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "id-length": 0,
      "no-console": 0,
      "no-unused-vars": [1, {"vars": "local", "args": "none"}],
      "strict": 0,
    },
}
